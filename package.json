{
  "packageManager": "pnpm@7.0.0",
  "scripts": {
    "start": "pnpm playground",
    "build": "pnpm run clean && pnpm build:icons && pnpm build:illustration && pnpm --filter @semcore/* --filter !@semcore/icon --filter !@semcore/illustration --filter !@semcore/ui run build",
    "process-theme": "tsm --require=./.ci/tsm-filter-warnings.js semcore/utils/theme/processor.ts",
    "typecheck": "pnpm --filter @semcore/* --filter !@semcore/babel-plugin-root --filter !@semcore/babel-plugin-styles --filter !@semcore/babel-plugin-react-semcore --filter !@semcore/babel-preset-ui --filter !@semcore/testing-utils --filter !@semcore/babel-plugin-recharts --filter !@semcore/email --filter !@semcore/dep-checker --filter !@semcore/code-mod --filter !@semcore/icon-transform-svg --filter !@semcore/shadow-loader --filter !@semcore/generator-component exec tsc --noEmit",
    "test": "vitest",
    "vo-test:setup": "npx @guidepup/setup",
    "vo-test": "playwright test --config playwright.vo.config.ts",
    "browser-test": "playwright test --config playwright.browser.config.ts",
    "website": "pnpm --dir website start",
    "lint": "pnpm lint:ts && pnpm lint:es && pnpm lint-changelogs",
    "lint-changelogs": "pnpm --filter @semcore/changelogs-handler run lint-changelogs",
    "publish:website": "tsm --require=./.ci/tsm-filter-warnings.js ./.ci/trigger-website-publish.ts",
    "publish:ui": "pnpm --filter @semcore/ui run update-changelog && pnpm install",
    "build:icons": "pnpm --filter @semcore/icon run build",
    "build:illustration": "pnpm --filter @semcore/illustration run build",
    "clean": "rm -rf semcore/*/lib && pnpm clean:icon && pnpm clean:illustration",
    "clean:icon": "pnpm --filter @semcore/icon run clean",
    "playground": "pnpm --filter @semcore/playground run start",
    "clean:illustration": "pnpm --filter @semcore/illustration run clean",
    "coverage": "vitest run --coverage",
    "format": "rome format ./semcore ./tools ./website ./.ci",
    "lint:es": "rome check ./semcore ./tools ./website ./.ci",
    "lint:ts": "tsc --project tsconfig.json --noEmit",
    "lint:css": "stylelint **/*.css",
    "prepare": "husky install",
    "postinstall": "pnpm --filter @semcore/ui run clear && pnpm run git:setup",
    "i18n:submit": "pnpm crowdin upload",
    "i18n:status": "pnpm crowdin status",
    "i18n:download": "pnpm crowdin download && tsm --require=./.ci/tsm-filter-warnings.js .ci/i18n-csv-2-json.ts",
    "git:setup": "git config --local attributesfile.name \".gitattributes\" && git config --local rebase.backend \"merge\" && git config --local merge.changelog-merge-driver.name \"changelog-merge-driver\" && git config --local merge.changelog-merge-driver.driver \"pnpm tsm --require=./.ci/tsm-filter-warnings.js ./.ci/changelog-merge-driver.ts %A %B\""
  },
  "private": true,
  "devDependencies": {
    "@babel/preset-react": "7.12.1",
    "@babel/runtime": "7.17.9",
    "@crowdin/cli": "3.10.1",
    "@guidepup/playwright": "0.6.1",
    "@jridgewell/set-array": "^1.1.2",
    "@mailchimp/mailchimp_marketing": "3.0.30",
    "@playwright/test": "1.25.1",
    "@semcore/babel-plugin-react-semcore": "workspace:*",
    "@semcore/babel-plugin-recharts": "workspace:*",
    "@semcore/babel-plugin-root": "workspace:*",
    "@semcore/babel-plugin-styles": "workspace:*",
    "@semcore/babel-preset-ui": "workspace:*",
    "@semcore/builder": "workspace:*",
    "@semcore/changelog-handler": "workspace:*",
    "@semcore/continuous-delivery": "workspace:*",
    "@semcore/core": "workspace:*",
    "@semcore/d3-chart": "workspace:*",
    "@semcore/esbuild-plugin-semcore": "workspace:*",
    "@semcore/gcs-upload": "workspace:*",
    "@semcore/slack-integration": "workspace:*",
    "@semcore/theme-redesign": "1.5.12",
    "@testing-library/react": "13.4.0",
    "@vitejs/plugin-react": "2.0.0",
    "@vitest/browser": "^0.32.0",
    "@vitest/coverage-v8": "^0.32.0",
    "@vitest/ui": "^0.32.0",
    "csv": "6.2.0",
    "dotenv": "8.2.0",
    "esbuild": "0.14.29",
    "execa": "6.1.0",
    "fast-glob": "3.2.11",
    "fast-hash-code": "^2.1.0",
    "fs-extra": "9.1.0",
    "happy-dom": "9.20.3",
    "husky": "8.0.0",
    "inquirer": "6.3.1",
    "lint-staged": "10.2.9",
    "picocolors": "1.0.0",
    "playwright": "1.25.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rome": "^12.1.3",
    "semver": "5.6.0",
    "simple-git": "3.16.0",
    "stylelint": "^15.9.0",
    "stylelint-config-recommended": "^12.0.0",
    "tsm": "2.2.2",
    "typescript": "4.5.5",
    "typescript-plugin-css-modules": "2.2.0",
    "typescript-workspace-plugin": "2.0.1",
    "vite-plugin-babel": "^1.1.3",
    "vitest": "^0.32.0",
    "yaml": "2.1.1"
  },
  "lint-staged": {
    "*.{css}": "pnpm lint:css --fix"
  },
  "resolutions": {
    "csstype": "3.0.8",
    "minimist": "1.2.6",
    "semver-regex": "3.1.3",
    "nth-check": "2.0.1",
    "trim-newlines": "3.0.1",
    "glob-parent": "6.0.2",
    "braces": "3.0.2",
    "@babel/runtime": "7.19.0",
    "@babel/core": "7.19.1",
    "axe-core": "4.4.0"
  },
  "engines": {
    "node": ">=16"
  }
}
