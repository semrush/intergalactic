{
  "packageManager": "pnpm@7.0.0",
  "scripts": {
    "start": "pnpm playground",
    "build": "pnpm build:icons && pnpm build:illustration && pnpm --filter @semcore/* --filter !@semcore/icon --filter !@semcore/ui run build",
    "build:icons": "pnpm --filter @semcore/icon run build",
    "build:illustration": "pnpm workspace @semcore/illustration build",
    "typecheck": "pnpm --filter @semcore/* --filter !@semcore/babel-plugin-root --filter !@semcore/babel-plugin-shadow --filter !@semcore/babel-plugin-styles --filter !@semcore/babel-plugin-react-semcore --filter !@semcore/babel-preset-ui --filter !@semcore/jest-preset-ui --filter !@semcore/babel-plugin-recharts --filter !@semcore/email --filter !@semcore/dep-checker --filter !@semcore/code-mod --filter !@semcore/icon-transform-svg --filter !@semcore/shadow-loader --filter !@semcore/generator-component exec tsc --noEmit",
    "prebuild": "rm -rf semcore/**/lib && pnpm prebuild:icon && pnpm prebuild:illustration",
    "prebuild:icon": "pnpm --filter @semcore/icon run prebuild",
    "prebuild:illustration": "pnpm --filter @semcore/illustration run prebuild",
    "test": "jest --no-cache",
    "vo-test:setup": "npx @guidepup/setup",
    "vo-test": "playwright test",
    "playground": "pnpm --filter @semcore/playground run start",
    "website": "pnpm --dir website start",
    "coverage": "jest --no-cache --coverage",
    "pub": "super-publisher --root ./semcore",
    "lint:es": "eslint -c .eslintrc --ext .ts,.tsx,.js,.jsx ./semcore",
    "lint:ts": "tsc --project tsconfig.json --noEmit",
    "lint": "pnpm lint:ts && pnpm lint:es",
    "prettier:fix": "pretty-quick --staged --ignore-path=./.prettierignore",
    "publish:website": "tsm --require=./.ci/tsm-filter-warnings.js ./.ci/trigger-website-publish.ts",
    "prepare": "husky install",
    "postinstall": "pnpm --filter @semcore/ui run clear",
    "i18n:submit": "pnpm crowdin upload",
    "i18n:status": "pnpm crowdin status",
    "i18n:download": "pnpm crowdin download && tsm --require=./.ci/tsm-filter-warnings.js .ci/i18n-csv-2-json.ts"
  },
  "private": true,
  "devDependencies": {
    "@crowdin/cli": "3.7.9",
    "@guidepup/playwright": "0.6.1",
    "@mailchimp/mailchimp_marketing": "3.0.30",
    "@semcore/babel-plugin-recharts": "workspace:*",
    "@semcore/babel-plugin-root": "workspace:*",
    "@semcore/babel-plugin-shadow": "workspace:*",
    "@semcore/babel-plugin-styles": "workspace:*",
    "@semcore/babel-preset-ui": "workspace:*",
    "@semcore/builder": "workspace:*",
    "@semcore/core": "workspace:*",
    "@semcore/d3-chart": "workspace:*",
    "@semcore/jest-preset-ui": "workspace:*",
    "@semcore/theme-redesign": "1.5.12",
    "@types/jest": "29.0.0",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "@typescript-eslint/parser": "5.10.2",
    "csv": "6.2.0",
    "dotenv": "8.2.0",
    "eslint": "8.14.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-import-resolver-workspaces": "1.0.9",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-no-cyrillic-string": "1.0.5",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-ssr-friendly": "1.1.0",
    "execa": "6.1.0",
    "husky": "8.0.0",
    "jest": "29.0.1",
    "jest-environment-jsdom": "29.0.1",
    "lerna": "4.0.0",
    "lint-staged": "10.2.9",
    "picocolors": "1.0.0",
    "playwright": "1.25.1",
    "prettier": "2.6.2",
    "pretty-quick": "1.11.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "tsm": "2.2.2",
    "typescript": "4.5.5",
    "typescript-plugin-css-modules": "2.2.0",
    "typescript-workspace-plugin": "2.0.1",
    "yaml": "2.1.1"
  },
  "jest": {
    "preset": "@semcore/jest-preset-ui"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": "eslint --fix"
  },
  "resolutions": {
    "csstype": "3.0.8",
    "minimist": "1.2.6",
    "semver-regex": "3.1.3",
    "nth-check": "2.0.1",
    "trim-newlines": "3.0.1",
    "glob-parent": "6.0.2",
    "braces": "3.0.2",
    "@babel/runtime": "7.17.9"
  }
}
