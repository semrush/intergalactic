{
  "packageManager": "pnpm@8.7.6",
  "scripts": {
    "dev": "pnpm --filter @semcore/playground run start",
    "start": "pnpm --filter @semcore/playground run start",
    "build": "pnpm run clean && pnpm --filter @semcore/core run build && pnpm build:icons && pnpm build:illustration && pnpm --filter @semcore/base-components run build && pnpm --filter @semcore/dropdown run build && pnpm --filter @semcore/* --filter !@semcore/icon --filter !@semcore/core --filter !@semcore/base-components --filter !@semcore/illustration --filter !@semcore/dropdown --filter !@semcore/ui run build",
    "process-theme": "tsm --require=./.ci/tsm-filter-warnings.js semcore/core/src/theme/processor.ts",
    "typecheck": "pnpm --filter @semcore/* --filter !@semcore/babel-plugin-root --filter !@semcore/babel-plugin-styles --filter !@semcore/babel-plugin-react-semcore --filter !@semcore/babel-preset-ui --filter !@semcore/testing-utils --filter !@semcore/email --filter !@semcore/dep-checker --filter !@semcore/icon-transform-svg --filter !@semcore/shadow-loader exec tsc --noEmit",
    "install-n-test": "pnpm install && pnpm test",
    "install-n-browser-test": "pnpm install && pnpm browser-test",
    "install-n-axe-test": "pnpm install && pnpm axe-test",
    "test": "vitest",
    "test:setup": "docker build -t intergalactic-playwright:16.0.0 -f playwright.dockerfile .",
    "test:docker": "pnpm run-in-docker pnpm install-n-test",
    "browser-test": "pnpm playwright test --config playwright.browser.config.ts",
    "browser-test:docker": "pnpm run-in-docker pnpm install-n-browser-test",
    "axe-test": "pnpm playwright test --config playwright.axe.config.ts",
    "axe-test:docker": "pnpm run-in-docker pnpm install-n-axe-test",
    "test:types": "vitest typecheck",
    "vo-test:setup": "npx @guidepup/setup",
    "vo-test": "playwright test --config playwright.vo.config.ts",
    "vo-test:ci": "pnpm tsm --require=./.ci/tsm-filter-warnings.js ./.ci/vo-tests-ci.ts",
    "run-in-docker": "docker run -e PLAYWRIGHT_ARGS= --rm --network host -v $(pwd)/package.json:/work/package.json -v $(pwd)/vite.config.ts:/work/vite.config.ts -v $(pwd)/playwright.browser.config.ts:/work/playwright.browser.config.ts -v $(pwd)/playwright.axe.config.ts:/work/playwright.axe.config.ts -v $(pwd)/.storybook:/work/.storybook -v $(pwd)/allure-results:/work/allure-results/ -v $(pwd)/semcore:/work/semcore/ -v $(pwd)/stories:/work/stories/ -v $(pwd)/tools:/work/tools/ -v $(pwd)/website:/work/website/ -v $(pwd)/test-results:/work/test-results/ -w /work/ intergalactic-playwright:16.0.0",
    "website": "pnpm --dir website start",
    "lint": "pnpm lint:es && pnpm lint-changelogs && pnpm lint:ts",
    "lint-changelogs": "pnpm --filter @semcore/changelog-handler run lint-changelogs",
    "publish:website": "pnpm --filter intergalacti—Å-migrate run build && tsm --require=./.ci/tsm-filter-warnings.js ./.ci/trigger-website-publish.ts",
    "build:icons": "pnpm --filter @semcore/icon run build",
    "build:illustration": "pnpm --filter @semcore/illustration run build",
    "clean": "rm -rf semcore/*/lib && pnpm clean:icon && pnpm clean:illustration",
    "clean:icon": "pnpm --filter @semcore/icon run clean",
    "clean:illustration": "pnpm --filter @semcore/illustration run clean",
    "format": "biome format ./semcore ./tools ./website ./stories ./.ci --config-path . --write",
    "format-changelogs": "tsm --require=./.ci/tsm-filter-warnings.js ./tools/continuous-delivery/bin/format-changelogs.ts",
    "lint:es": "biome check ./semcore ./tools ./website ./.ci --config-path .",
    "lint:ts": "tsc --noEmit && cd website && tsc --noEmit",
    "lint:css": "stylelint **/*.css",
    "lint:docs": "vale README.md CONTRIBUTING.md semcore/*/README.md semcore/*/CHANGELOG.md tools/*/README.md tools/*/CHANGELOG.md website/docs",
    "prepare": "husky install",
    "postinstall": "pnpm --filter @semcore/ui run clear && pnpm run git:setup",
    "i18n:submit": "pnpm crowdin upload",
    "i18n:status": "pnpm crowdin status",
    "i18n:download": "pnpm crowdin download && tsm --require=./.ci/tsm-filter-warnings.js .ci/i18n-csv-2-json.ts",
    "git:setup": "git config --local attributesfile.name \".gitattributes\" && git config --local rebase.backend \"merge\" && git config --local merge.changelog-merge-driver.name \"changelog-merge-driver\" && git config --local merge.changelog-merge-driver.driver \"pnpm tsm --require=./.ci/tsm-filter-warnings.js ./.ci/changelog-merge-driver.ts %A %B\"",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "tsm --require=./.ci/tsm-filter-warnings.js ./.ci/prebuild-stories.ts && storybook build",
    "storybook:test": "storybook test"
  },
  "private": true,
  "devDependencies": {
    "@babel/preset-react": "7.12.1",
    "@babel/runtime": "7.17.9",
    "@biomejs/biome": "1.2.2",
    "@crowdin/cli": "3.10.1",
    "@guidepup/playwright": "0.13.2",
    "@jridgewell/set-array": "1.1.2",
    "@playwright/test": "1.48.0",
    "@semcore/babel-plugin-react-semcore": "workspace:*",
    "@semcore/babel-plugin-root": "workspace:*",
    "@semcore/babel-plugin-styles": "workspace:*",
    "@semcore/babel-preset-ui": "workspace:*",
    "@semcore/builder": "workspace:*",
    "@semcore/changelog-handler": "workspace:*",
    "@semcore/continuous-delivery": "workspace:*",
    "@semcore/core": "workspace:*",
    "@semcore/d3-chart": "workspace:*",
    "@semcore/esbuild-plugin-semcore": "workspace:*",
    "@semcore/gcs-upload": "workspace:*",
    "@semcore/icon": "workspace:*",
    "@semcore/slack-integration": "workspace:*",
    "@semcore/theme-redesign": "1.5.12",
    "@storybook/addon-a11y": "8.3.5",
    "@storybook/addon-essentials": "8.3.5",
    "@storybook/addon-interactions": "8.3.5",
    "@storybook/addon-links": "8.3.5",
    "@storybook/addon-storysource": "8.3.5",
    "@storybook/blocks": "8.3.5",
    "@storybook/manager-api": "8.4.6",
    "@storybook/react": "8.3.5",
    "@storybook/react-vite": "8.3.5",
    "@storybook/test": "8.3.5",
    "@testing-library/react": "13.4.0",
    "@types/glob": "^8.1.0",
    "@types/node": "18.16.15",
    "@types/react": "18.0.21",
    "@vitejs/plugin-react": "2.0.0",
    "@vitest/browser": "3.0.6",
    "@vitest/ui": "3.0.6",
    "allure-commandline": "2.32.0",
    "allure-js-commons": "3.0.9",
    "allure-playwright": "3.0.9",
    "allure-vitest": "3.0.9",
    "csv": "6.2.0",
    "dotenv": "8.2.0",
    "esbuild": "0.14.29",
    "execa": "6.1.0",
    "fast-glob": "3.2.11",
    "fast-hash-code": "2.1.0",
    "fs-extra": "9.1.0",
    "glob": "7.1.4",
    "http-server": "14.1.1",
    "husky": "8.0.0",
    "inquirer": "6.3.1",
    "intergalactic-migrate": "workspace:*",
    "jsdom": "22.1.0",
    "lint-staged": "10.2.9",
    "picocolors": "1.0.0",
    "playwright": "1.48.0",
    "playwright-core": "1.48.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rollup-plugin-copy": "3.5.0",
    "semver": "5.6.0",
    "simple-git": "3.16.0",
    "storybook": "8.3.5",
    "stylelint": "15.9.0",
    "stylelint-config-recommended": "12.0.0",
    "tsm": "2.2.2",
    "typescript": "5.1.6",
    "typescript-plugin-css-modules": "2.2.0",
    "typescript-workspace-plugin": "2.0.1",
    "unplugin": "1.14.1",
    "vite": "5.4.9",
    "vite-plugin-babel": "1.1.3",
    "vitest": "3.0.6",
    "webdriverio": "8.24.3",
    "yaml": "2.1.1"
  },
  "lint-staged": {
    "*.{css}": "pnpm lint:css --fix",
    "*": "tsm --require=./.ci/tsm-filter-warnings.js ./.ci/lint-internals.ts"
  },
  "resolutions": {
    "minimist": "1.2.6",
    "semver-regex": "3.1.3",
    "nth-check": "2.0.1",
    "trim-newlines": "3.0.1",
    "glob-parent": "6.0.2",
    "braces": "3.0.2",
    "@babel/runtime": "7.19.0",
    "@babel/core": "7.19.1",
    "axe-core": "4.9.1",
    "ansi-regex": "5.0.1",
    "@types/scheduler": "< 0.23.0"
  },
  "engines": {
    "node": ">=16"
  },
  "pnpm": {
    "patchedDependencies": {
      "parse-es-import@0.6.0": "patches/parse-es-import@0.6.0.patch",
      "react-virtualized@9.22.3": "patches/react-virtualized@9.22.3.patch",
      "react-hook-form@7.54.0": "patches/react-hook-form@7.54.0.patch"
    }
  }
}
