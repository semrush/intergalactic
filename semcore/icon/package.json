{
  "name": "@semcore/icon",
  "description": "SEMRush Icon Component",
  "version": "2.26.1",
  "main": "lib/cjs/index.js",
  "module": "lib/es6/index.js",
  "typings": "lib/types/index.d.ts",
  "sideEffects": false,
  "author": "Roman Lysov <r.lysov@semrush.com>",
  "license": "MIT",
  "scripts": {
    "parsing": "icon-transform-svg parsing",
    "transform": "icon-transform-svg --sourceFolder=svg-new --outputFolder=.",
    "transform:old-icon": "icon-transform-svg --sourceFolder=svg --outputFolder=lib --deprecated-warning",
    "build": "build --source=js && npm run transform:old-icon && npm run transform",
    "test": "jest"
  },
  "dependencies": {
    "@semcore/utils": "^3.15",
    "@semcore/flex-box": "^4",
    "classnames": "2.2.6"
  },
  "peerDependencies": {
    "@semcore/core": "^1.11",
    "react": "16.8 - 17"
  },
  "jest": {
    "preset": "@semcore/jest-preset-ui"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/semrush/intergalactic.git",
    "directory": "semcore/icon"
  },
  "devDependencies": {
    "@semcore/jest-preset-ui": "1.0.0"
  }
}
