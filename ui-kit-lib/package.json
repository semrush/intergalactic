{
  "name": "intergalactic",
  "version": "15.32.0",
  "license": "MIT",
  "module": "./index.mjs",
  "sideEffects": false,
  "scripts": {
    "prepublishOnly": "pnpm run clear && pnpm run release",
    "postinstall": "pnpm run checkImports",
    "clear": "tsm --require=../.ci/tsm-filter-warnings.js tools/clear.ts",
    "release": "pnpm run updateChangelog && pnpm run copyLibs",
    "copyLibs": "tsm --require=../.ci/tsm-filter-warnings.js tools/copyLibs.ts",
    "checkImports": "tsm --require=../.ci/tsm-filter-warnings.js tools/checkImports.ts",
    "patchImports": "tsm --require=../.ci/tsm-filter-warnings.js tools/patchImports.ts",
    "updateChangelog": "tsm --require=../.ci/tsm-filter-warnings.js tools/updateChangelog.ts"
  },
  "bin": {
    "check-imports": "tools/checkImports.ts",
    "patch-imports": "tools/patchImports.ts"
  },
  "exports": {
    "./ui/*": {
      "types": "./libs/*/lib/types/index.d.ts",
      "main": "./libs/*/lib/cjs/index.js",
      "module": "./libs/*/lib/es6/index.js"
    },
    "./*": {
      "types": "./libs/*/lib/types/index.d.ts",
      "main": "./libs/*/lib/cjs/index.js",
      "module": "./libs/*/lib/es6/index.js"
    }
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "16.8 - 18",
    "react-dom": "16.8 - 18"
  },
  "devDependencies": {
    "@semcore/continuous-delivery": "*",
    "@semcore/testing-utils": "1.0.0",
    "@types/fs-extra": "9.0.13",
    "@types/react": "18.0.21",
    "fast-glob": "3.2.11",
    "fs-extra": "9.0.1",
    "tsm": "2.2.2",
    "@ast-grep/napi": "0.13.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/semrush/intergalactic.git",
    "directory": "ui-kit-lib"
  }
}
