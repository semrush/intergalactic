{
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "lib": ["dom", "es6", "es7"],
    "jsx": "react",
    "declaration": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "sourceMap": true,
    "importHelpers": false,
    "noEmitHelpers": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "noImplicitAny": true,
    "paths": {
      "@semcore/ui/*": ["../../semcore/*"]
    },
    "typeRoots": ["types"]
  },
  "exclude": ["node_modules", "builder", "tools"]
}
